{
	"name":"Scikit-Learn KNN Classifier",
	"execution context":"transforms/model_transforms/sklearn_transforms/sklearn_base_transform.py",
	"dependencies":
	{
		"scikit-learn":
		{
			"language":"python"
		}
	},
	"params":
	{
		"module name":"sklearn.neighbors.KNeighborsClassifier",
		"mode":["generator", "executor"]
	},
	"hyperparameters":
	{
		"n_neighbors":
		{
			"type":"int",
			"constraints":"",
			"default":"5"
		},
		"weights":
		{
			"type":"string",
			"constraints":["uniform","distance"],
			"default":"uniform"
		},
		"algorithm":
		{
			"type":"string",
			"constraints":["ball_tree","kd_tree","brute","auto"],
			"default":"auto"
		},
		"leaf_size":
		{
			"type":"int",
			"constraints":"",
			"default":"30"
		},
		"metric":
		{
			"type":"string",
			"constraints":["minkowski"],
			"default":"minkowski"
		},
		"p":
		{
			"type":"int",
			"constraints":"",
			"default":"2"
		}
	},
	"inputs":
	{
		"datax":
		{
			"fmt":["csv"],
			"file":"",
			"description":"Input Training Data Features"
		},
		"datay":
		{
			"fmt":["csv"],
			"file":"",
			"description":"Input Training Data Labels"
		},
		"model":
		{
			"fmt":["pickle"],
			"file":"",
			"description":"Serialized Pre-trained Model Input for Warm-Start"
		}
	},
	"outputs":
	{
		"datax":
		{
			"fmt":["csv"],
			"file":"",
			"description":"Output Transformed Data Features"
		},
		"datay":
		{
			"fmt":["csv"],
			"file":"",
			"description":"Output Predicted Labels"
		},
		"model":
		{
			"fmt":["pickle"],
			"file":"",
			"description":"Serialized Trained Model Output by Generator"
		}
	},	
	"functions":
	{
		"Simple Generator":
		{
			"params":
			{
				"mode":"generator"
			},
			"outputs":
			{
				"datax":
				{
					"fmt":[""],
					"file":[""]
				},
				"datay":
				{
					"fmt":[""],
					"file":[""]
				}
			},
			"description":"Generates a serialized KNN model from training data and labels"
		},
		"Simple Executor":
		{
			"params":
			{
				"mode":"executor"
			},
			"outputs":
			{
				"model":
				{
					"fmt":[""],
					"file":[""]
				},
				"datax":
				{
					"fmt":[""],
					"file":[""]
				}
			},
			"inputs":
			{
				"datay":
				{
					"fmt":[""],
					"file":[""]
				}
			},
			"description":"Generates labels from a serialized model input and testing data"
		}
	}
}
