{
	"Template":"Scikit-Learn Random Forest Classifier Transform",
	"PrimaryExec":"ProtoML-transforms/transforms/model_transforms/sklearn_transforms/sklearn_base_transform.py",
	"Dependencies":
	{
		"scikit-learn":
		{
			"language":"python"
		}
	},
	"PrimaryParameters": {
		"module name": {
			"Default": "sklearn.ensemble.RandomForestClassifier",
			"Description": "The scikit-learn module to run"
		},
		"mode": {
			"Default": "generator",
			"Description": "The mode selection between generator and executor"
		}
	},
	"PrimaryHyperParameters":
	{
		"n_estimators":
		{
			"Type":"int",
			"Default":"10",
			"Constraints":""
		},
		"criterion":
		{
			"Type":"string",
			"Default":"gini",
			"Constraints":["gini","entropy"]
		},
		"max_features":
		{
			"Type":["int","real","string"],
			"Default":"auto",
			"Constraints":[[":int","=","max_features",""],[":real","[]","0","1"],[":string","=","max_features","auto","sqrt","log2",""]]
		},
		"max_depth":
		{
			"Type":"int",
			"Default":"",
			"Constraints":""
		},
		"min_samples_split":
		{
			"Type":"int",
			"Default":"2",
			"Constraints":""
		},
		"min_samples_leaf":
		{
			"Type":"int",
			"Default":"1",
			"Constraints":""
		},
		"bootstrap":
		{
			"Type":"bool",
			"Default":"1",
			"Constraints":""
		},
		"oob_score":
		{
			"Type":"bool",
			"Default":"0",
			"Constraints":""
		},
		"n_jobs":
		{
			"Type":"int",
			"Default":"1",
			"Constraints":[["=","n_jobs","-1"],["[]","1","$NCPU"]]
		},
		"random_state":
		{
			"Type":"int",
			"Default":"",
			"Constraints":""
		},
		"verbose":
		{
			"Type":"int",
			"Default":"0",
			"Constraints":""
		}
	},
	"PrimaryInputs":
	{
		"datax":
		{
			"Type":["number"],
			"Format":["csv"],
			"Description":"Input Training Data Features"
		}
	},
	"PrimaryOutputs":
	{	
		"datay":
		{
			"Type":["number"],
			"Format":["csv"],
			"Description":"Output Predicted Labels"
		}
	},	
	"PrimaryInputStates":
	{
		"model":
		{
			"Format":["pickle"],
			"Description":"Serialized Pre-trained Model Input for Warm-Start",
			"Optional":"true"
		}
	},
	"PrimaryOutputStates":
	{
		"model":
		{
			"Format":["pickle"],
			"Description":"Serialized Trained Model Output by Generator"
		}
	},
	"Functions": {
		"Generator": {
			"Description":"Generates a serialized KNN model from training data and labels",
			"Inputs": {		
				"datay":
				{
					"Type":["number"],
					"Format":["csv"],
					"Description":"Input Data Labels"
				}
			},
			"Outputs": {
				"datay": []
			}
		},
		"Executor": {
			"Parameters": {
				"mode": "executor"
			},
			"Description":"Generates labels from a serialized model input and testing data",	
			"InputStates": {
				"model": {
					"Optional": false
				}
			},
			"OutputStates": {
				"model": []
			}
		}
	}	
}
	
