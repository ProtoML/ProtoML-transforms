{
	"Template":"Scikit-Learn SGD Classifier Transform",
	"PrimaryExec":"ProtoML-transforms/transforms/model_transforms/sklearn_transforms/sklearn_base_transform.py",
	"Dependencies":
	{
		"scikit-learn":
		{
			"language":"python"
		}
	},
	"PrimaryParameters":
	{
		"module name":["sklearn.linear_model.SGDClassifier"],
		"mode":["generator","executor"]
	},
	"PrimaryHyperParameters":
	{
		"loss":
		{
			"Type":"string",
			"Default":"hinge",
			"Constraints":[["=","loss","log","hinge","modified_huber","squared_hinge","perceptron","squared_loss","huber","epsilon_insensitive","squared_epsilon_insensitive"]]
		},
		"penalty":
		{
			"Type":"string",
			"Default":"l2",
			"Constraints":[["=","penalty","l2","l1","elasticnet"]]
		},
		"alpha":
		{
			"Type":"real",
			"Default":"0.0001",
			"Constraints":""
		},
		"l1_ratio":
		{
			"Type":"real",
			"Default":"0.15",
			"Constraints":[["[]","0","1"]]
		},
		"fit_intercept":
		{
			"Type":"bool",
			"Default":"1",
			"Constraints":""
		},
		"n_iter":
		{
			"Type":"int",
			"Default":"5",
			"Constraints":""
		},
		"shuffle":
		{
			"Type":"bool",
			"Default":"0",
			"Constraints":""
		},
		"random_state":
		{
			"Type":"int",
			"Default":"",
			"Constraints":""
		},
		"epsilon":
		{
			"Type":"real",
			"Default":"0.1",
			"Constraints":[["=","loss","huber","epsilon_insensitive","squared_epsilon_insensitive"]],
			"_comment":"Example of dependent Constraints: will check to ensure that loss' value is in there"
		},
		"n_jobs":
		{
			"Type":"int",
			"Default":"1",
			"Constraints":[["=","n_jobs","-1"],["[]","1","$NCPU"]]
		},
		"learning_rate":
		{
			"Type":"string",
			"Default":"optimal",
			"Constraints":[["=","learning_rate","constant","optimal","invscaling"]]
		},
		"eta0":
		{
			"Type":"real",
			"Default":"0.01",
			"Constraints":""
		},
		"power_t":
		{
			"Type":"real",
			"Default":"0.5",
			"Constraints":""
		},
		"class_weight":
		{
			"Type":"string",
			"Default":"",
			"Constraints":[["=","class_weight","","auto"]]
		},
		"warm_start":
		{
			"Type":"bool",
			"Default":"0",
			"Constraints":["=","warm_start","0"]
		}
	},
	"PrimaryInputs":
	{
		"datax":
		{
			"Format":["csv"],
			"Path":"",
			"Description":"Input Training Data Features"
		},
		"datay":
		{
			"Format":["csv"],
			"Path":"",
			"Description":"Input Training Data Labels"
		}	
	},
	"PrimaryOutputs":
	{
		"datax":
		{
			"Format":["csv"],
			"Path":"",
